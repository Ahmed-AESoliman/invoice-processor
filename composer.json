{
  "name": "ahmed-soliman/invoice-processor",
  "description": "A library to import and process invoices from Excel",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "ahmed abdelazim"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.2",
    "phpoffice/phpspreadsheet": "^1.25"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "scripts": {
    "test": "phpunit",
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "php -S localhost:8000 -t public/"
    ]
  },
  "config": {
    "sort-packages": true
  }
}
